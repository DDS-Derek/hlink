{
  "name": "@hlink/app",
  "version": "2.0.0-next.0",
  "type": "module",
  "main": "./dist/app.js",
  "module": "./dist/app.js",
  "exports": "./dist/app.js",
  "types": "./dist/app.d.ts",
  "scripts": {
    "dev": "run-p dev:* dev:server:*",
    "dev:app": "vite --host",
    "dev:server:watch": "tsc -w --project ./server/tsconfig.json",
    "dev:server:start": "nodemon -V -i ./configs ./dist/index.js",
    "server": "run-p dev:server:*",
    "build": "rm -rf dist && run-p build:*",
    "build:client": "tsc && vite build",
    "preview": "node dist/index.js",
    "build:server": "tsc --project ./server/tsconfig.json",
    "np": "np --no-cleanup --no-tests --no-yarn",
    "np:next": "np --tag=next --any-branch --yolo --no-yarn"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@hlink/core": "workspace:^2.0.0-next.0",
    "@koa/router": "^10.1.1",
    "@types/koa__router": "^8.0.11",
    "@types/lodash": "^4.14.182",
    "ansi-html": "^0.0.9",
    "antd": "^4.21.0",
    "chalk": "^5",
    "classnames": "^2.3.1",
    "execa": "^6.1.0",
    "fs-extra": "^10",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-compress": "^5.1.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "lodash-id": "^0.14.1",
    "lowdb": "^3.0.0",
    "monaco-editor": "^0.33.0",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^1.3.0",
    "user-startup": "^0.2.2"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-static": "^4.0.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@unocss/preset-attributify": "^0.38.1",
    "@unocss/preset-uno": "^0.38.1",
    "@unocss/preset-wind": "^0.38.1",
    "@unocss/vite": "^0.38.1",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-plugin-import": "^1.13.5",
    "less": "^4.1.3",
    "unocss": "^0.38.1",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imp": "^2.2.0"
  }
}
